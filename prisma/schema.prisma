generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Blog {
id      String @id @default(uuid())
title   String
message String
user_id String
user User @relation(fields: [user_id],references: [id])
}

model User {
  id       String @id @default(uuid())
  username String @unique
  email    String @unique
  password String
  blog Blog[]
}

